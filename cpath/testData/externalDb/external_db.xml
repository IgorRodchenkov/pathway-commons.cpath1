<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE external_database_list SYSTEM "external_db.dtd">
<external_database_list>
    <!-- 
    ###################################################################
    A Sample Set of External Databases
    ###################################################################    
    -->
    
    <external_database type="LINK_OUT">
        <name>Yahoo</name>
        <description>Yahoo Search</description>
        <url>
            <url_pattern>http://search.yahoo.com/search?p=%ID%</url_pattern>
            <sample_id>test</sample_id>
        </url>
        <controlled_terms>
            <master_term>YAHOO</master_term>
            <synonym>YHO</synonym>
        </controlled_terms>
    </external_database>

    <external_database type="LINK_OUT">
        <name>Google</name>
        <description>Google Search</description>
        <url>
            <url_pattern>http://www.google.com/search?q=%ID%</url_pattern>
            <sample_id>NF01438533</sample_id>
        </url>
        <controlled_terms>
            <master_term>GOOGLE</master_term>
            <synonym>GOOGLE WEB</synonym>
        </controlled_terms>
    </external_database>
</external_database_list>